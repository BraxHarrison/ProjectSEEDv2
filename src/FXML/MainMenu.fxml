
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:controller="Display.MainMenuUI" xmlns:fx="http://javafx.com/fxml" alignment="center">
    <VBox fx:id="MenuArea" StackPane.alignment="CENTER" maxHeight="80" maxWidth="80" spacing="10">
        <Button fx:id="newGameButton" text="New Game" onAction="#triggerNewGame"/>
        <Button fx:id="loadGameButton" text = "Load Game" onAction="#triggerLoadGame"/>
    </VBox>
</StackPane>